#! /usr/bin/env node

var dox = require("dox");
var fs = require("fs");
var argv = require("yargs");

var args = argv
  .usage('matterfront-docgen [options]')
  .describe('build','builds docs for given files')
  .array('build')
  .example('build','matterfront-docgen --build lib/commands/info.js lib/utils builds docs for lib/commands/info.js and all files under lib/utils')
  .help('h')
  .argv;


var input = Rx.Observable.pairs(args);
